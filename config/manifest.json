{
    "file_type": "csv",
    "name": "Inupiaq Phrasebook",
    "display": "phrase",
    "compare": "compare_form",
    "sorting": "sort_form",
    "transducers": [
        {
            "source": "phrase",
            "target": "compare_form",
            "functions": [
                "../transducers/inupiaq-approx.yaml"
            ]
        },
        {
            "source": "phrase",
            "target": "sort_form",
            "functions": [
                "lambda x: x.lower()"
            ]
        }
    ],
    "targets": {
        "entryID": "A",
        "phrase": "B",
        "definition": "C",
        "audio": [
            {
                "speaker": "D",
                "variety": "E",
                "filename": "G"
            }
        ],
        "theme": "F",
        "optional": [
            {
                "Pronunciation": "6"
            }
        ]
    }
}
